<tr>
  <td>
  </td>
  <td>
    <%= user.name %>
  </td>
  <td>
    <%= user.email %>
  </td>
  <td>
    <% if user.supervisor? %>
      <%= t ".supervisor" %>
    <% else %>
      <%= t ".trainee" %>
    <% end %>
  </td>
  <td>
    <%= link_to edit_user_registration_path(user_id: user.id),
      class: "btn btn-primary btn-link btn-sm",rel: "tooltip", title: t(".edit") do %>
       <i class="material-icons">edit</i>
    <% end %>
    <%= link_to user_registration_path(user_id: user.id), method: :delete,
      data: {confirm: t(".confirm")}, class: "btn btn-danger btn-link btn-sm",rel: "tooltip",title: t(".delete") do %>
       <i class="material-icons">close</i>
    <% end %>
  </td>
</tr>
